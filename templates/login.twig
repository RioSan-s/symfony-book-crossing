<html>
<head>
    <meta charset="UTF-8">
    {%  block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
<h1>Аутентификация</h1>

<div class="col-3">
    {{ form(form_login) }}
    {% if errors %}
        <div>
            {{ errors.messageKey |trans(errors.messageData, 'security')}}
        </div>
    {% endif %}
</div>

</body>
{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</html>